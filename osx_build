#Make headers
javac ./com/coherentpdf/Jcpdf.java -h .
#Build shared library
cc -shared -fpic -I../cpdflib-source -lcpdf -I$JAVA_HOME/include -I$JAVA_HOME/include/darwin jcpdfwrapper.c -o libjcpdf.dylib
#Copy shipping component to test/
cp libjcpdf.dylib test/
cp ../cpdflib-source/libcpdf.so libcpdf.dylib
javac ./com/coherentpdf/Jcpdf.java
javac ./com/coherentpdf/Testjcpdf.java
jar cf coherentpdf.jar com
